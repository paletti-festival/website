<h2>Termine</h2>

<div class="biglietti-list">
{%- for post in collections.dates -%}
    <div class="biglietti">
        <div class="row">
            <span class="title">{{ '/images/paletti_icon_inverted.svg' | svgContents | safe}} {{ post.data.title }}</span>
        </div>
        <div class="row">
            <div class="date-time">
                <span class="date">{{ post.data.date.toLocaleDateString('de-DE', {day: "2-digit", month: "short", year: "numeric"}) }}</span>
                <span class="time">
                    {{ post.data.date.toLocaleTimeString('de-DE', {hour: "2-digit", minute: "2-digit"}) }} â€“
                    {{ post.data.until.toLocaleTimeString('de-DE', {hour: "2-digit", minute: "2-digit"}) }}
                </span>
            </div>
            <span class="text">
                Eintritt frei<br>
                Hallo Spende
            </span>
        </div>
        <div class="row">
            <span class="address">
                {{ post.data.address }}
            </span>
        </div>
    </div>
{%- endfor -%}
</div>
