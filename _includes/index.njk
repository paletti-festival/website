---
layout: layout
---
<section>
    {{ content | safe }}
</section>
{%- for post in collections.main | reverse -%}
{% if post.data.lang == page.lang %}
    <section
    {% if post.data.class %}
    class="{{ post.data.class }}"
    {% endif %}
    >
    {{ post.content | safe}}
    </section>
{% endif %}
{%- endfor -%}